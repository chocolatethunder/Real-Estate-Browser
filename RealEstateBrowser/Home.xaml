<Window x:Class="RealEstateBrowser.Home"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RealEstateBrowser"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox Margin="120,144,192,145" TextWrapping="Wrap" Text="Enter a location ..." FontSize="14" FontStyle="Italic"/>
        <Button Margin="367,144,120,0" VerticalAlignment="Top" Height="30" Click="Button_Click">
            <Button.Background>
                <ImageBrush ImageSource="pack://siteoforigin:,,,/Resources/gps.png"/>
            </Button.Background>
        </Button>
        <Button Content="Go" HorizontalAlignment="Left" Margin="330,144,0,0" VerticalAlignment="Top" Width="32" Height="30" FontSize="16"/>

    </Grid>
</Window>
